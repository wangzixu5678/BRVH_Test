<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.ftkj.baserecyclerviewadapterhelper_test">

    <application
        android:name=".recycle_shopping.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:theme="@style/WelcomeTheme"
            android:name=".WelcomActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".recycle_anim.AnimActivity"/>
        <activity android:name=".recycle_drag.DragActivity"/>
        <activity android:name=".recycle_shopping.ShoppingActivity"/>
        <activity android:name=".recycle_multiple.MultipleActivity"/>
        <activity android:name=".recycle_multiple.MultipleActivity2"/>
        <activity android:name=".recycle_section.SectionActicity"/>
        <activity android:name=".recycle_refresh_swipe.RefreshSwipActivity"/>
        <activity android:name=".recycle_expandable.ExpandableActivity"/>
    </application>

</manifest>